paths="DFN-Cernet-SP DFN-Unicom-SP Versatel-Cernet-SP Versatel-Unicom-SP DFN-Cernet-CMT DFN-Unicom-CMT Versatel-Cernet-CMT Versatel-unicom-CMT DFN-Cernet-mptcp DFN-Unicom-mptcp Versatel-Cernet-mptcp Versatel-Unicom-mptcp"

SP="DFN-Cernet-SP DFN-Unicom-SP Versatel-Cernet-SP Versatel-Unicom-SP"
CMT="DFN-Cernet-CMT DFN-Unicom-CMT Versatel-Cernet-CMT Versatel-unicom-CMT"
MPTCP="DFN-Cernet-mptcp DFN-Unicom-mptcp Versatel-Cernet-mptcp Versatel-Unicom-mptcp"

DFNCERNET="DFN-Cernet-SP DFN-Cernet-CMT DFN-Cernet-mptcp"
DFNUNICOM="DFN-Unicom-SP DFN-Unicom-CMT DFN-Unicom-mptcp"
VERSATELCERNET="Versatel-Cernet-SP Versatel-Cernet-CMT Versatel-Cernet-mptcp"
VERSATELUNICOM="Versatel-Unicom-SP Versatel-unicom-CMT Versatel-Unicom-mptcp"


simulationFolder="/media/storage/svn-git/projects/sctp/sctp-misc/trunk/netperfmeter/src/hakim-tests/haikou-paper/essen-haikou/"
file="/Results/passive.flow-ReceivedByteRate.data"


function extractFile
{
  bzip2 -d  $1.bz2
}

function recompressFile
{
  bzip2 -f  $1
}



for onePath in $DFNCERNET ; do
  fileName="$simulationFolder$onePath$file"
  echo "extracting file from $onePath"
  extractFile  $fileName
  
  echo "editing file from $onePath"
  newFileName="tmpfile.tmp"
  
  cat $fileName  | \sed -e 's/bothSides/DFN-Cernet/g' > $newFileName
  mv $newFileName $fileName
  
  echo "recompressing file from $onePath"
  recompressFile $fileName
done


for onePath in $DFNUNICOM ; do
  fileName="$simulationFolder$onePath$file"
  echo "extracting file from $onePath"
  extractFile  $fileName
  
  echo "editing file from $onePath"
  newFileName="tmpfile.tmp"
  
  cat $fileName  | \sed -e 's/bothSides/DFN-Unicom/g' > $newFileName
  mv $newFileName $fileName
  
  echo "recompressing file from $onePath"
  recompressFile $fileName
done

for onePath in $VERSATELCERNET ; do
  fileName="$simulationFolder$onePath$file"
  echo "extracting file from $onePath"
  extractFile  $fileName
  
  echo "editing file from $onePath"
  newFileName="tmpfile.tmp"
  
  cat $fileName  | \sed -e 's/bothSides/Versatel-Cernet/g' > $newFileName
  mv $newFileName $fileName
  
  echo "recompressing file from $onePath"
  recompressFile $fileName
done

for onePath in $VERSATELUNICOM ; do
  fileName="$simulationFolder$onePath$file"
  echo "extracting file from $onePath"
  extractFile  $fileName
  
  echo "editing file from $onePath"
  newFileName="tmpfile.tmp"
  
  cat $fileName  | \sed -e 's/bothSides/Versatel-Unicom/g' > $newFileName
  mv $newFileName $fileName
  
  echo "recompressing file from $onePath"
  recompressFile $fileName
done

for onePath in $SP ; do
  fileName="$simulationFolder$onePath$file"
  echo "extracting file from $onePath"
  extractFile  $fileName
  
  echo "editing file from $onePath"
  newFileName="tmpfile.tmp"
  
  cat $fileName  | \sed -e 's/off/Single-Path/g' > $newFileName
  mv $newFileName $fileName
  
  echo "recompressing file from $onePath"
  recompressFile $fileName
done

for onePath in $CMT ; do
  fileName="$simulationFolder$onePath$file"
  echo "extracting file from $onePath"
  extractFile  $fileName
  
  echo "editing file from $onePath"
  newFileName="tmpfile.tmp"
  
 cat $fileName  | \sed -e 's/like-mptcp/CMT-SCTP/g' > $newFileName
 mv $newFileName $fileName
  
  echo "recompressing file from $onePath"
  recompressFile $fileName
done


for onePath in $MPTCP ; do
  fileName="$simulationFolder$onePath$file"
  echo $fileName
  echo "extracting file from $onePath"
  extractFile  $fileName
  
  echo "editing file from $onePath"
  newFileName="tmpfile.tmp"
  
 cat $fileName  | \sed -e 's/off/MPTCP/g' > $newFileName
 mv $newFileName $fileName
  
  echo "recompressing file from $onePath"
  recompressFile $fileName
done



#OptionBufferSplitting  bothsides==>path
#CMTCCVariant like-mptcp==> CMT-SCTP




 